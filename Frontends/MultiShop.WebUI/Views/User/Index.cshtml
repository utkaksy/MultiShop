@model UserDetailViewModel
@{
    Layout = null;
}

<link href="~/lib/bootstrap/dist/css/bootstrap.rtl.min.css" rel="stylesheet" />

<div>
    <br /><br /><br />
    <h3>Kullanıcı Profil Bilgisi</h3>
    <div class="alert alert-info">Kullanıcı Id:@Model.Id</div>
    <div class="alert alert-info">Kullanıcı Adı:@Model.Username</div>
    <div class="alert alert-info">Ad Soyad:@Model.Name @Model.Surname</div>
    <div class="alert alert-info">Email Adresi:@Model.Name @Model.Email</div>
</div>